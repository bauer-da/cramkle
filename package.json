    "@types/ramda": "types/npm-ramda#dist",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^1.4.1",
    "@typescript-eslint/parser": "^1.4.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
