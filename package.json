    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1",
    "graphql-resolvers": "^0.3.2",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^4.0.4",
    "mongoose": "^5.3.8",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
